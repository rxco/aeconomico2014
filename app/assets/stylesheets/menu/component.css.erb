/* if an icon anchor has a span, hide the span */

.gn-icon span {
	position: absolute;
	left: 8px;
	top: 15px;
	font-size: 12px;
}

.gn-icon-menu::before {
		margin-left: -2px;
		vertical-align: 6px;
		width: 34px;
		height: 3px;
		background: #292929;
		box-shadow: 0 3px white, 0 -6px #292929, 0 -9px white, 0 -12px #292929;
		content: '';
}

.no-touch .gn-icon-menu:hover::before,
.no-touch .gn-icon-menu.gn-selected:hover::before {
		background: white;
		box-shadow: 0 3px #d23138, 0 -6px white, 0 -9px #d23138, 0 -12px white;
}

