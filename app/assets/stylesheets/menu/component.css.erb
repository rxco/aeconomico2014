.gn-menu-main,
.gn-menu-main ul {
		margin: 0;
		padding: 0;
		background: white;
		color: #292929;
		list-style: none;
		text-transform: none;
		font-weight: 300;
		font-family: 'Aller-Light', sans-serif;
		line-height: 60px;
		text-transform: uppercase;
		z-index: 3;
}

.gn-menu-main {
		position: fixed;
		top: 0;
		left: 0;
		width: 100%;
		height: 60px;
		-webkit-box-shadow: 0 2px 10px rgba(0, 0, 0, .175);
          box-shadow: 0 2px 10px rgba(0, 0, 0, .175);
}

.gn-menu-main a {
		display: block;
		height: 100%;
		color: #292929;
		text-decoration: none;
		cursor: pointer;
